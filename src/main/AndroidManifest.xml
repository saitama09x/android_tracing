<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.android"
    android:versionCode="1"
    android:versionName="1.0.0" >
	
	    <uses-sdk 
              android:targetSdkVersion="24"
               />
			  
	<uses-permission android:name="android.permission.INTERNET" />  
	<uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />	

    <application android:label="I Trace I Learn"
    	android:icon="@mipmap/mainlogo"
    	android:theme="@style/Theme.AppCompat.Light.NoActionBar"
    	>
        <activity
            android:name="com.android.controller.HomePage"
            android:label="I Trace I Learn"

			android:screenOrientation="landscape">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

		<activity android:name="com.android.controller.CategoryPage" 
		android:screenOrientation="landscape"
		android:theme="@style/Theme.AppCompat.Light.NoActionBar"/>
		
		<activity android:name="com.android.controller.Practice_tracing" 
		android:screenOrientation="landscape"
		android:theme="@style/Theme.AppCompat.Light.NoActionBar"/>

		<activity android:name="com.android.controller.evaluation.Evaluation" 
		android:screenOrientation="landscape"
		android:theme="@style/Theme.AppCompat.Light.NoActionBar"/>
	
		<activity android:name="com.android.controller.MainSettings" 
		android:screenOrientation="landscape"
		android:theme="@style/Theme.AppCompat.Light.NoActionBar"/>

		<activity android:name="com.android.controller.Student_score" 
		android:screenOrientation="landscape"
		android:theme="@style/Theme.AppCompat.Light.NoActionBar"/>


